.base-input-style {
	background-color #f00
	border 0
	color #f00
	display block
	font-family 'Arial'
	font-size 22px
	padding 15px
	text-transform uppercase
}

icon-font-size = 20px

.i-wrap {
	display block
	margin 0 auto

	> input {
		border-radius 0
		height 56px
		line-height 56px
		padding-bottom 0
		padding-top 0
	}

	> input,
	> textarea {
		width 100%
	}

	&.search-input {
		display block
		position relative

		> input {
			padding-right 60px
		}

		&:after {
			color #f00
			font-size 24px
			margin-top -20px
			position absolute
			right 20px
			top 50%
		}
	}
}

.s-wrap {
	display block
	height 60px
	position relative

	> select {
		display block
		height 100%
		left 0
		position absolute
		top 0
		width 100%
		z-index 2

		.js & {
			-moz-appearance window // hide select arrow on firefox 30
			background-color transparent
			background-image none
			border 0
			box-shadow none

			&::-ms-expand {
				display none // hide select arrow on ie 11
			}

			&:focus {
				outline none
			}
		}

		&:focus + .s-value {
			font-size 20px
		}

		&,
		&:hover,
		option,
		option:hover {
			font-size 20px
		}

		option:hover {
			background-color darken(#f00, 10%)
		}

		> button {
			border 10px solid red
		}
	}

	&.s-highlight {
		.s-value {
			background-color #f00
			color #f00

			&:after {
				color #f00
			}
		}
	}

	.s-value {
		bottom 0
		height 60px
		left 0
		line-height 60px
		overflow hidden
		padding-bottom 0
		padding-right 35px
		padding-top 0
		position absolute
		right 0
		text-overflow ellipsis
		top 0
		white-space nowrap
		z-index 1

		&:after {
			margin-top -2px
			position absolute
			right 20px
			top 50%
		}
	}

	&.error,
	.error & {
		.s-value {
			border-color red

			&:focus {
				font-size 20px-error
			}
		}
	}
}

